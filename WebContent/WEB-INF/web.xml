<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <servlet>
  <servlet-name>come</servlet-name>
  <servlet-class>com.mp.Book</servlet-class>  
  </servlet>
  
  <servlet-mapping>
  <servlet-name>come</servlet-name>
  <url-pattern>/book</url-pattern>
  </servlet-mapping>
  
  <servlet>
  <servlet-name>go</servlet-name>
  <servlet-class>com.mp.BookDisplay</servlet-class>  
  </servlet>
  
  <servlet-mapping>
  <servlet-name>go</servlet-name>
  <url-pattern>/cod</url-pattern>
  </servlet-mapping>
   <servlet>
    <servlet-name>login</servlet-name>
    <servlet-class>com.mp.Login</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>login</servlet-name>
    <url-pattern>/Login</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>register</servlet-name>
    <servlet-class>com.mp.Register</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>register</servlet-name>
    <url-pattern>/Register</url-pattern>
  </servlet-mapping>

</web-app>